{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev-championFan": "npm run clear && cross-env APP_KEY=championFan CONFIG_KEY=dev vite --open",
    "dev-default": "npm run clear && cross-env APP_KEY=default CONFIG_KEY=dev vite --open",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cspell": "npx cspell \"**\"",
    "clear": "clear || cls",
    "push": "npm run cspell && npm run clear && git add -A && npx opencommit && git push"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "clear": "^0.1.0",
    "cross-env": "^7.0.3",
    "cspell": "^8.9.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "opencommit": "^3.0.16",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "vite": "^5.3.1",
    "vite-plugin-html": "^3.2.2"
  }
}